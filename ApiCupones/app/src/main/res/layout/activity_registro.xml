<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/edNombre"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="9dp"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="231dp"
        android:layout_marginBottom="578dp"
        android:ems="10"
        android:enabled="true"
        android:hint="Nombre"
        android:inputType="text" />

    <EditText
        android:id="@+id/edApellidoPaterno"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="213dp"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="578dp"
        android:ems="10"
        android:enabled="true"
        android:hint="Apellido Paterno"
        android:inputType="text" />

    <EditText
        android:id="@+id/edEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="7dp"
        android:layout_marginTop="247dp"
        android:layout_marginEnd="232dp"
        android:layout_marginBottom="438dp"
        android:ems="10"
        android:enabled="true"
        android:hint="Correo"
        android:inputType="text" />

    <EditText
        android:id="@+id/edDireccion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="6dp"
        android:layout_marginTop="309dp"
        android:layout_marginEnd="232dp"
        android:layout_marginBottom="376dp"
        android:ems="10"
        android:enabled="true"
        android:hint="Direccion"
        android:inputType="text" />

    <EditText
        android:id="@+id/edPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="9dp"
        android:layout_marginTop="389dp"
        android:layout_marginEnd="229dp"
        android:layout_marginBottom="296dp"
        android:ems="10"
        android:enabled="true"
        android:hint="ContraseÃ±a"
        android:inputType="text" />

    <EditText
        android:id="@+id/edTelefono"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="208dp"
        android:layout_marginTop="182dp"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="504dp"
        android:ems="10"
        android:enabled="true"
        android:hint="Telefono"
        android:inputType="text" />

    <EditText
        android:id="@+id/edApellidoMaterno"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="179dp"
        android:layout_marginEnd="231dp"
        android:layout_marginBottom="506dp"
        android:ems="10"
        android:enabled="true"
        android:hint="Apellido Materno"
        android:inputType="text" />


    <EditText
        android:id="@+id/edFechaNacimiento"
        android:layout_width="192dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="205dp"
        android:layout_marginTop="251dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="434dp"
        android:enabled="true"
        android:focusable="false"
        android:hint="Fecha de nacimiento"
        android:inputType="none" />

    <Button
        android:id="@+id/btnFechaNacimiento"
        android:layout_width="199dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="205dp"
        android:layout_marginTop="354dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="318dp"
        android:backgroundTint="@color/purple"
        android:enabled="true"
        android:text="Seleccionar Fecha" />

    <Button
        android:id="@+id/btnRegistrar"
        android:layout_width="308dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="55dp"
        android:layout_marginTop="653dp"
        android:layout_marginEnd="47dp"
        android:layout_marginBottom="24dp"
        android:backgroundTint="@color/purple"
        android:text="Aceptar"
        android:textSize="18sp"
        app:cornerRadius="20dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="262dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="80dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="69dp"
        android:layout_marginBottom="670dp"
        android:text="Crea tu cuenta"
        android:textAlignment="center"
        android:textColor="@color/purple"
        android:textColorHighlight="#673AB7"
        android:textSize="34sp" />

    <ImageView
        android:id="@+id/ivRegresar"
        android:layout_width="69dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="6dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="336dp"
        android:layout_marginBottom="678dp"
        app:srcCompat="@drawable/arrow_back_24" />


</RelativeLayout>